-include /opt/build-harness/Makefile.prow

.PHONY: build
build: ## Build the docker image.
	docker build -f Dockerfile . -t search-v2-api

.PHONY: lint
lint:
	build/run-lint-check.sh
	gosec ./...

.PHONY: unit-test
unit-test:
	go test ./... -v -coverprofile cover.out
